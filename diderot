import sys
from diderot_core import DiderotCLI

if __name__ == '__main__':
    if len(sys.argv) > 1:
        # TODO: get rid of this ability from the student accessible version.
        DiderotCLI().cmdloop(sys.argv[1])
    else:
        DiderotCLI().cmdloop("https://www.diderot.one")
